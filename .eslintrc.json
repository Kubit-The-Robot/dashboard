{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "react/no-children-prop": "off",
    "no-unused-vars": "warn",
    "no-use-before-define": ["error", { "functions": false }],
    "prettier/prettier": ["error"]
  }
}
